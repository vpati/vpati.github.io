---
# Automatically generated by bib2md.py
title: "{{ title }}"
slug: "{{ ID }}"
{% if timestamp is defined %}date: "{{ timestamp }}"{% endif %}
publishdate: "2000-01-01"

{% if author is sequence %}authors:
{% for a in author %}  - "{{ a.firstname }} {{ a.lastname }}"
{% endfor %}{% endif %}
category: {{ kind }}
{% if "conference" == kind or "workshop" == kind  %}venue: "{{ booktitle['name'] }}{% if series %} ({{ series['name']}}){% endif %}"{% endif %}
{% if "journal" == kind %}venue: "{{ journal['name'] }} (Volume {{ volume }}, Issue {{ number }})"{% endif %}
{% if "magazine" == kind %}venue: "{{ journal['name'] }}"{% endif %}
slides: {{ slides }}
paper: {{ paper }}
text: {{ text }}
{% if url %}external_url: {{ url }}{% endif %}
{% if video_url %}video_url: {{ video_url }}{% endif %}
{% if publishnote %}publishnote: {{ publishnote }}{% endif %}
bibtex: |
    {{ bibtex|indent(width=4) }}

{% if aliases or crossref %}
aliases:
{% if crossref %}- "publications/{{ crossref }}"{% endif %}
{% for alias in aliases %}- "publications/{{ alias }}"
{% endfor %}{% endif %}
---

{{ abstract }}
